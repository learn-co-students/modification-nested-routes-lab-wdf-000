<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.hidden_field :artist_id %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= artist_id_field(@song) %>
  <% f.label :edit %>
  <% display_artist(@song) %>
  <%= f.submit %>
<% end %>
